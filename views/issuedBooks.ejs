<head>
    <title>Issued Books</title>
    <link rel="stylesheet" type="text/css" href="../css/style1.css">
    </head>
<body>

<h3><%= msg %></h3>
<table>
    <tr><th>ID</th><th>Book ID</th><th>User ID</th><th>Issued Date</th><th>Return Date</th><th>Period</th><th>Fine</th></tr>

<% books.forEach(function(item){ %>
    <tr>
        <td><%= item.ib_id %></td>
        <td><%= item.b_id %></td>
        <td><%= item.u_id %></td>
        <td><%= item.issued_date %></td>
        <td><%= item.return_date %></td>
        <td><%= item.period %></td>
        <td><%= item.fine %></td>
    </tr>
    <% }); %>
</table>
</body>